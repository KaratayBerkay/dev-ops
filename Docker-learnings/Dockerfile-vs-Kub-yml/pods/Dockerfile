FROM ubuntu:latest
# FROM image to pull from docker.hub
LABEL authors="User"
# LABEL variables that are match as variables
ENTRYPOINT ["top", "-b"]
# ENTRYPOINT is the first command line that will be used as a start point o cmd
CMD ["10"]
# A command line that will be applied right after entry point
# top -b 10 will be executed when container is up&running
